{
  "name": "bookmark-search",
  "description": "Search engine for bookmarks filtering",
  "version": null,
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "bookmark-search",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  },
  "permissions": [
    "http://google.com/",
    "http://127.0.0.1/",
    "http://127.0.0.1:8000/*",
    "cookies",
    "bookmarks"
  ]
}
